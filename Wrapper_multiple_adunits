    function refreshBid() {
        console.log("Cybermedia")
        pbjs.que.push(function() {
            pbjs.requestBids({
                timeout: PREBID_TIMEOUT,
                adUnitCodes: ['/21902364955/CMHB_tutuapp.me'],
                bidsBackHandler: function() {
                    pbjs.setTargetingForGPTAsync(['/21902364955/CMHB_tutuapp.me']);
                    googletag.pubads().refresh([slot1]);
                }
            });
        });
        pbjs.que.push(function() {
            pbjs.requestBids({
                timeout: PREBID_TIMEOUT,
                adUnitCodes: ['/21902364955/CMHB_tutuapp.me/CMHB_tutuapp.me_ATF_300x250'],
                bidsBackHandler: function() {
                    pbjs.setTargetingForGPTAsync(['/21902364955/CMHB_tutuapp.me/CMHB_tutuapp.me_ATF_300x250']);
                    googletag.pubads().refresh([slot2]);
                    
                }
            });
        });
            pbjs.que.push(function() {
            pbjs.requestBids({
                timeout: PREBID_TIMEOUT,
                adUnitCodes: ['/21902364955/CMHB_tutuapp.me/CMHB_tutuapp.me_ATF2_300x250'],
                bidsBackHandler: function() {
                    pbjs.setTargetingForGPTAsync(['/21902364955/CMHB_tutuapp.me/CMHB_tutuapp.me_ATF2_300x250']);
                    googletag.pubads().refresh([slot3]);
                    
                }
            });
        });
            pbjs.que.push(function() {
