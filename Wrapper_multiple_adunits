}];

pbjs.que.push(function() {
    pbjs.addAdUnits(adUnits);
    pbjs.setConfig({ debug: true });
    pbjs.setConfig({
        sizeConfig: [{
            'mediaQuery': '(min-width: 1025px)',
            'sizesSupported': desktopSizes,
            'labels': ['desktop']
        }, {
            'mediaQuery': '(min-width: 768px) and (max-width: 1024px)',
            'sizesSupported': tabletSizes,
            'labels': ['tablet']
        }, {
            'mediaQuery': '(min-width: 320px) and (max-width: 812px)',
            'sizesSupported': phoneSizes,
            'labels': ['phone']
        }]
    });
});

var slot1;

googletag.cmd.push(function() {
    var mapping = googletag.sizeMapping().

addSize([992, 0], [[300, 250]]). //desktop

addSize([768, 0], [[728, 90]]). //tablet

addSize([320, 0], [[300, 250]]). //mobile

addSize([0, 0], [[320, 250]]). //other

build();

slot1 = googletag.defineSlot('/21902364955/CMHB_tutuapp.me', [[300, 250]], 'div-gpt-ad-1582716038426-0').defineSizeMapping(mapping).addService(googletag.pubads());   
googletag.pubads().enableSingleRequest();
googletag.enableServices();  
});
