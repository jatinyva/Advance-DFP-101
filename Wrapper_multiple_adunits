    [300, 250]
];
var div_1_sizes = [
    [300, 250]
];
var div_2_sizes = [
    [300, 600]
];

var PREBID_TIMEOUT = 3000;

var googletag = googletag || {};
googletag.cmd = googletag.cmd || [];

var pbjs = pbjs || {};
pbjs.que = pbjs.que || [];

var adUnits = [{
